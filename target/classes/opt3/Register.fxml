<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="opt3.Controller.RegisterController">
   <children>
      <TextField fx:id="usernameInput" layoutX="219.0" layoutY="108.0" promptText="Gebruikersnaam">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="emailInput" layoutX="219.0" layoutY="205.0" promptText="Email">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </TextField>
      <Text layoutX="226.0" layoutY="78.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Registreer je hier">
         <font>
            <Font name="Arial" size="19.0" />
         </font>
      </Text>
      <PasswordField fx:id="passwordInput" layoutX="219.0" layoutY="157.0" promptText="Wachtwoord">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </PasswordField>
      <Button fx:id="registerButton" layoutX="216.0" layoutY="257.0" mnemonicParsing="false" onAction="#OnRegisterButtonClick" text="Registreren">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="50.0" right="50.0" top="10.0" />
         </padding>
      </Button>
      <Button fx:id="backButton" layoutX="67.0" layoutY="59.0" mnemonicParsing="false" onAction="#OnBackClick" text="Terug">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
